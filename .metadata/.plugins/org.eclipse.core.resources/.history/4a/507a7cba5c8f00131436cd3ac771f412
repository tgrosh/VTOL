package com.singletongames.vtol;

import org.andengine.extension.physics.box2d.PhysicsFactory;

import com.badlogic.gdx.physics.box2d.BodyDef.BodyType;
import com.singletongames.vtol.Util.BodyShape;

public class WoodenBox extends PhysicsSprite {

	public WoodenBox(float pX, float pY) {		
		super(pX, pY, Resources.WoodenBox, PhysicsFactory.createFixtureDef(.05f, 0, .2f), "WoodenBox", BodyType.DynamicBody, BodyShape.Box, null);
	}

}
